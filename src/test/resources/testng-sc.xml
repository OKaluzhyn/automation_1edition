<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="StandartCheck" verbose="1">

   

    <test name="ChatTest">
        <parameter name="site" value="http://edubirdie.com"/>
        <classes>
            <class name="ua.qa.edusson.tests.standartCheck.ChatTests"/>
        </classes>
    </test>

    <test name="essayvikingsTest">
        <parameter name="site" value="http://essayvikings.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.standartCheck.ChooseBestWriterVasPayPalTests"/>
