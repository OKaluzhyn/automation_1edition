<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="StandartCheck" verbose="1" >

    <test name="Edusson" >
        <parameter name="site" value="http://edusson.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.StandartCheckEdussonProductionTests" />
            </classes>
    </test>

    <test name="Chat" >
        <parameter name="site" value="http://edubirdie.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.ChatTests" />
        </classes>
    </test>

    <test name="Edubirdie" >
        <parameter name="site" value="http://edubirdie.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.StandartCheckNotEasyBiddingSitesEdussonGatewayTests" />
        </classes>
    </test>

    <test name="Papersowl" >
        <parameter name="site" value="http://papersowl.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.StandartCheckNotEasyBiddingSitesEdussonGatewayTests" />
        </classes>
    </test>

    <test name="Studymoose" >
        <parameter name="site" value="http://essays.studymoose.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.StandartCheckOnlyEasyBiddingSitesPayPalTests" />
        </classes>
    </test>

    <test name="Customwriting" >
        <parameter name="site" value="http://customwriting.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.StandartCheckOnlyEasyBiddingSitesPayPalTests" />
        </classes>
    </test>

    <test name="Essayvikings" >
        <parameter name="site" value="http://essayvikings.com/"/>
        <classes>
            <class name="ua.qa.edusson.tests.StandartCheckVasChooseBestWritersPayPalTests" />
        </classes>
    </test>

</suite>
